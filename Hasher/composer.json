{
    "name": "vixel-framework/hasher",
    "description": "A secure password hasher.",
    "keywords": [],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/vixel-framework/vixel-framework/issues",
        "source": "https://github.com/vixel-framework/vixel-framework"
    },
    "require": {
        "php": ">=8.0",
        "paragonie/constant_time_encoding": "^2.4",
        "symfony/options-resolver": "^5.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "vimeo/psalm": "^4"
    },
    "autoload": {
        "psr-4": { "Vixel\\Hasher\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "optimize-autoloader": true
    },
    "prefer-stable": true
}
